infJumpDebounce = false


InfJump.Name = "InfJump"
InfJump.Parent = Frame
InfJump.BackgroundColor3 = Color3.fromRGB(0,0,0)
InfJump.Position = UDim2.new(-0.001, 0,0.504, 0)
InfJump.Size = UDim2.new(0, 223,0, 62)
