BloodGui.MouseButton1Click:Connect(function()
	if isFast then
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
	else
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 30
	end
	isFast = not isFast
end)

