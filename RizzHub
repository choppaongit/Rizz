HigherJump.TextScaled = true
HigherJump.TextSize = 14.000
HigherJump.TextWrapped = true

local isHigherJump = false

local function toggleHigherJump()
	if isHigherJump then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50 
	else
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100 
	end
	isHigherJump = not isHigherJump
end

HigherJump.MouseButton1Click:Connect(toggleHigherJump)

AutoToxic.Name = "AutoToxic"
AutoToxic.Parent = Frame
AutoToxic.BackgroundColor3 = Color3.fromRGB(0,0,0)
AutoToxic.Position = UDim2.new(0.685, 0,0.306, 0)
AutoToxic.Size = UDim2.new(0, 223,0, 62)
AutoToxic.Font = Enum.Font.Sarpanch
AutoToxic.Text = "AutoToxic"
AutoToxic.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoToxic.TextScaled = true
AutoToxic.TextSize = 14.000
AutoToxic.TextWrapped = true

local isAutoToxicEnabled = false -- track whether the AutoToxic feature is enabled or not
local autoToxicCoroutine -- reference to the coroutine that sends the message repeatedly

-- function that sends the "Blackerson On Top" message repeatedly
local function sendAutoToxicMessage()
	while isAutoToxicEnabled do
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("imagine playing " .. GameName ..  " Get Rizzed With Rizz Hub", "All")
		wait(2)
	end
end

-- handle the AutoToxic button click
AutoToxic.MouseButton1Click:Connect(function()
	isAutoToxicEnabled = not isAutoToxicEnabled -- toggle the feature status

	if isAutoToxicEnabled then
		-- start the coroutine if it's not running
		if not autoToxicCoroutine then
			autoToxicCoroutine = coroutine.create(sendAutoToxicMessage)
			coroutine.resume(autoToxicCoroutine)
		end
	else
		-- stop the coroutine if it's running
		if autoToxicCoroutine then
			coroutine.yield(autoToxicCoroutine)
			autoToxicCoroutine = nil
		end
	end
end)

local isNoclipping = false
local noclipConnection

local infJump
infJumpDebounce = false

Uninject.Name = "Uninject"
Uninject.Parent = Frame
Uninject.BackgroundColor3 = Color3.fromRGB(0,0,0)
Uninject.Position = UDim2.new(0.314, 0,0.889, 0)
Uninject.Size = UDim2.new(0, 223,0, 62)
